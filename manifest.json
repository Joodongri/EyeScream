{
  "manifest_version": 2,
  "name": "EyeScream",
  "description": "codesmith chrome extension project",
  "version": "1.0",
  "icons": {"128": "logo_128.png"},
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["style/popup.css"],
    "js": ["script/jquery-3.4.1.min.js", "script/content.js"]
  }]
}